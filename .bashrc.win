alias gitpr='EDITOR="C:/Program Files/Git/usr/bin/vim" git push && hub pull-request'
alias gitmfm='git pull && git merge origin/master'
alias dotnetcb='git clean -fdx & git rm **/packages.lock.json & dotnet build'
